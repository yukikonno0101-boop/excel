{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": ".vercel/output",
    "jsx": "react-jsx"
  },
  "include": ["api/**/*.ts", "**/*.tsx", "**/*.ts"],
  "exclude": ["node_modules"]
}
